{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DeployTown Config",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the val"
    },
    "type": {
      "type": "string",
      "enum": ["script", "http"],
      "description": "The type of the val"
    },
    "privacy": {
      "type": "string",
      "enum": ["public", "private", "unlisted"],
      "description": "The privacy of the val"
    },
    "entry": {
      "type": "string",
      "description": "Entry point file path"
    }
  },
  "required": ["name", "type", "privacy", "entry"]
}
